<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE xml>
<odoo>
    <data>

        <menuitem id = "hotel_menu" name = "Hotel"/>

        <menuitem id = "management_menu" name = "Hotel Management" parent = "hotel_menu"/>

        <menuitem id = "management_menu1" name = "Hotel Management" parent = "management_menu" action = "hotel_action"/>

        <record id = "hotel" model = "ir.ui.view" >
            <field name = "name">hotel</field>
            <field name = "model">hmshotel.hmshotel</field>
            <field name = "arch" type = "xml">
                <tree>
                    <field name = "name" String = "Name"/>
                    <field name = "code" String = "location"/>
                    <field name = "location" String = "Location"/>
                </tree>
            </field>
        </record>


        <record id = "hotel_action" model = "ir.actions.act_window">
            <field name = "name">hotel_action</field>
            <field name = "res_model">hmshotel.hmshotel</field>
            <field name = "view_mode">tree,form></field>
        </record>

        

    </data>
</odoo>